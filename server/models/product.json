{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "invoices": {
      "type": "hasMany",
      "model": "Invoice",
      "foreignKey": "productId"
    },
    "types": {
      "type": "embedsOne",
      "model": "Type",
      "property": "type",
      "foreignKey": "typeId",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "categories": {
      "type": "embedsOne",
      "model": "Category",
      "property": "category",
      "foreignKey": "categoryId",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
